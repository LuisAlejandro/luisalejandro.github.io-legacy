{
  "name": "luisalejandro.github.io",
  "version": "0.2.0",
  "description": "Personal site",
  "author": "Luis Alejandro Mart√≠nez Faneyth",
  "repository": "LuisAlejandro/luisalejandro.github.io",
  "scripts": {
    "serve_development": "export NODE_ENV=development && gulp serve:development",
    "serve_production": "export NODE_ENV=production && gulp serve:production",
    "build_development": "export NODE_ENV=development && gulp build:development",
    "build_production": "export NODE_ENV=production && gulp build:production",
    "postinstall": "gulp libraries:gems:common libraries:fonts:common",
    "snyk-protect": "npx snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "jquery-bridget": "./node_modules/jquery-bridget/jquery-bridget.js",
    "anime": "./node_modules/animejs/anime.js",
    "vue": "./node_modules/vue/dist/vue.js",
    "vue-router": "./node_modules/vue-router/dist/vue-router.js",
    "howler": "./node_modules/howler/dist/howler.js"
  },
  "browserify-shim": {
    "jquery": "global:jQuery",
    "jquery-bridget": "global:jQueryBridget",
    "anime": "global:anime",
    "vue": "global:Vue",
    "vue-router": "global:VueRouter",
    "howler": "global:Howl"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "babelify",
      "vueify"
    ]
  },
  "browserslist": [
    "last 2 versions"
  ],
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "semi": [
        2,
        "always"
      ]
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "compact": false
  },
  "devDependencies": {
    "animejs": "3.0.0",
    "audiosprite": "0.7.1",
    "babel-core": "6.26.3",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babelify": "8.0.0",
    "bootstrap": "4.3.1",
    "browser-sync": "2.26.6",
    "browserify": "16.2.2",
    "browserify-shim": "3.8.14",
    "css-mqpacker": "7.0.0",
    "cssnano": "4.0.5",
    "del": "3.0.0",
    "eslint": "5.4.0",
    "eslint-config-standard": "12.0.0-alpha.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "4.0.0",
    "eslint-plugin-standard": "3.1.0",
    "fancy-log": "1.3.2",
    "google-font-downloader": "1.0.5",
    "gulp": "4.0.2",
    "gulp-base64": "0.1.3",
    "gulp-eslint": "6.0.0",
    "gulp-filter": "6.0.0",
    "gulp-flatten": "0.4.0",
    "gulp-google-webfonts": "2.0.0",
    "gulp-htmlmin": "5.0.1",
    "gulp-imagemin": "7.1.0",
    "gulp-inject": "5.0.5",
    "gulp-jsonlint": "1.2.1",
    "gulp-jsonminify": "1.1.0",
    "gulp-plumber": "1.2.0",
    "gulp-postcss": "8.0.0",
    "gulp-pretty-data": "0.1.2",
    "gulp-real-favicon": "0.3.0",
    "gulp-rename": "2.0.0",
    "gulp-replace": "1.0.0",
    "gulp-rev": "9.0.0",
    "gulp-rev-collector": "1.3.1",
    "gulp-sass": "4.0.1",
    "gulp-size": "3.0.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-svgstore": "7.0.1",
